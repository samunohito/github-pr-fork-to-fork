{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false
    },
    "baseUrl": "src",
    "target": "es2022",
    "externalHelpers": true,
    "paths": {
      "@proxy-spec/*": [
        "*"
      ]
    }
  },
  "module": {
    "type": "es6",
    "strict": true,
    "strictMode": true,
    "noInterop": false,
    "lazy": false,
    "resolve": {
      "alias": {
        "@proxy": "*"
      }
    }
  },
  "minify": false,
  "sourceMaps": "inline"
}
